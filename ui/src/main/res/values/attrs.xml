<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="AVLoadingIndicatorView">
        <attr name="minWidth" format="dimension" />
        <attr name="maxWidth" format="dimension"/>
        <attr name="minHeight" format="dimension" />
        <attr name="maxHeight" format="dimension"/>
        <attr name="indicatorName" format="string"/>
        <attr name="indicatorColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="SwipeMenuLayout">
        <attr name="swipeEnable" format="boolean"/>
        <attr name="ios" format="boolean"/>
        <attr name="leftSwipe" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="StrongTabLayout">
        <attr name="selectedIndicatorWidth" format="dimension" />
    </declare-styleable>
    <declare-styleable name="PlaceholderDraweeView">
        <attr name="placeholderIconWidth" format="dimension" />
        <attr name="placeholderIconHeight" format="dimension" />
        <attr name="placeholderIconBgColor" format="color" />
        <attr name="placeholderIconId" format="reference" />
    </declare-styleable>
</resources>